{
	"name": "import-to-require-devel",
	"displayName": "Import To Require Syntax (Devel)",
	"description": "Convert ES6 module import syntax to ES5 require syntax",
	"version": "0.6.0",
	"engines": {
		"vscode": "^1.27.2"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.replaceAllImports",
		"onCommand:extension.replaceAllExports"
	],
	"main": "./extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.replaceAllImports",
				"title": "Convert Imports to Require"
			},
			{
				"command": "extension.replaceAllExports",
				"title": "Convert ES6 Export to CJS Export"
			}
		],
		"keybindings": [
			{
				"command": "extension.replaceAllImports",
				"key": "ctrl+alt+m",
				"mac": "ctrl+alt+m",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.replaceAllExports",
				"key": "ctrl+alt+p",
				"mac": "ctrl+alt+p",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.21",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}